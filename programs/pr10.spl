for(fib1 = 0, fib2 = 0; !streams[0].empty; fib1 = fib2, fib2 = sum){
    
    /*
    Syntax for updating variables and declaring them is the same
    unlike in java, this coupled with global scope means we can
    simply write sum like this instead of having to declare "sum = 0"
    before the loop or in the initialisation expression as you would have
    to in java
    */
    sum = fib1 + fib2 + streams[0].take;
    print(sum);

};

end;